    <AssemblyName>WixGamingExtension</AssemblyName>
    <OutputType>Library</OutputType>
    <RootNamespace>Microsoft.Tools.WindowsInstallerXml.Extensions</RootNamespace>
    <FileUpgradeFlags>
    </FileUpgradeFlags>
    <OldToolsVersion>2.0</OldToolsVersion>
